{
  "name": "Unnamed AI Calendar Assistant",
  "description": "Helps with various tasks in Google Calendar",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "sidePanel",
    "tabs"
  ],
  "action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "scripts/sidePanelToggle.js"
  },
  "side_panel": {
    "default_path": "popup/popup.html"
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/start.js"
      ]
    }
  ]
}