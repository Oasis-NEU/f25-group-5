{
  "name": "Unnamed AI Calendar Assistant",
  "description": "Helps with various tasks in Google Calendar",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/start.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "scripts/moduleLoaderTest.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "scripts/helloWorld.js"
      ],
      "matches": [
        "<all_urls>" 
      ]
    }
  ]
}